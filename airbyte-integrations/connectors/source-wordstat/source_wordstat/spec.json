{
  "documentationUrl" : "https://docs.airbyte.io/connector-development",
  "connectionSpecification" : {
    "$schema" : "http://json-schema.org/draft-07/schema#",
    "title" : "Wordstat spec",
    "type" : "object",
    "required" : [
      "cookies",
      "region",
      "device",
      "keyword",
      "date_range",
      "group_by"
    ],
    "additionalProperties" : false,
    "properties" : {
      "cookies" : {
        "type" : "string",
        "title" : "Куки",
        "description" : "Куки, полученные из консоли разработчика в браузере в формате json",
        "airbyre_secret" : true,
        "order" : 0,
        "examples" : [
          "{\"yandexuid\":\"111222333\", \"Session_id\": \"10\"}"
        ]
      },
      "region" : {
        "title" : "Фильтр по регионам",
        "type" : "object",
        "order" : 1,
        "description" : "Локации, по которым будет осуществляться поиск",
        "oneOf" : [
          {
            "title" : "Все",
            "description" : "Искать во всех регионах",
            "properties" : {
              "region_filter" : {
                "type" : "string",
                "const" : "all",
                "order" : 0
              }
            },
            "required" : [
              "region_filter"
            ]
          }
        ],
        "default" : {
          "region_filter" : "all"
        }
      },
      "device" : {
        "title" : "Тип устройства",
        "order" : 2,
        "type" : "array",
        "items" : {
          "enum": [
            "desktop",
            "phone",
            "tablet"
          ]
        }
      },
      "keyword" : {
        "type" : "string",
        "title" : "Ключевое слово",
        "description" : "Ключевое слово для поиска",
        "order" : 3,
        "examples" : [
          "Холодильник",
          "Купить квартиру"
        ]
      },
      "date_range" : {
        "title" : "Диапазон дат поиска",
        "description" : "Выбрать даты, за которые будет осуществляться поиск",
        "type" : "object",
        "order" : 4,
        "oneOf" : [
          {
            "type" : "object",
            "title" : "Пользовательский интервал",
            "required" : [
              "date_from",
              "date_to",
              "date_range_type"
            ],
            "properties" : {
              "date_from" : {
                "title" : "Дата начала",
                "type" : "string",
                "description" : "Дата начала в формате YYYY-MM-DD.",
                "pattern" : "^$|^[0-9]{4}-[0-9]{2}-[0-9]{2}$",
                "pattern_descriptor" : "YYYY-MM-DD",
                "examples" : [
                  "2023-01-01"
                ],
                "format" : "date",
                "order" : 0
              },
              "date_to" : {
                "title" : "Дата конца",
                "type" : "string",
                "description" : "Дата конца в формате YYYY-MM-DD.",
                "pattern" : "^$|^[0-9]{4}-[0-9]{2}-[0-9]{2}$",
                "examples" : [
                  "2023-01-30"
                ],
                "pattern_descriptor" : "YYYY-MM-DD",
                "format" : "date",
                "order" : 1
              },
              "date_range_type" : {
                "type" : "string",
                "title" : "Тип временного диапазона",
                "description" : "Пользовательский интервал",
                "const" : "custom_date",
                "order" : 2
              }
            }
          },
          {
            "type" : "object",
            "title" : "Последние N дней",
            "required" : [
              "last_days_count",
              "should_load_today",
              "date_range_type"
            ],
            "properties" : {
              "last_days_count" : {
                "title" : "Число дней",
                "type" : "integer",
                "description" : "Число дней, не включая сегодня",
                "minimum" : 0,
                "maximum" : 3650,
                "examples" : [
                  30
                ],
                "order" : 0
              },
              "should_load_today" : {
                "title" : "Загружать сегодня?",
                "type" : "boolean",
                "description" : "Загружать данные за сегодня? Если нет, последней датой загрузки будет вчера",
                "order" : 1,
                "default" : false
              },
              "date_range_type" : {
                "type" : "string",
                "title" : "Тип временного диапазона",
                "description" : "Последние N дней",
                "const" : "last_n_days",
                "order" : 2
              }
            }
          },
          {
            "type" : "object",
            "title" : "От начальной даты до сегодня",
            "description" : "Загружать данные от начальной даты по сегодняшнее число",
            "required" : [
              "date_from",
              "should_load_today",
              "date_range_type"
            ],
            "properties" : {
              "date_from" : {
                "title" : "Дата начала",
                "type" : "string",
                "description" : "Дата начала в формате YYYY-MM-DD.",
                "pattern" : "^$|^[0-9]{4}-[0-9]{2}-[0-9]{2}$",
                "pattern_descriptor" : "YYYY-MM-DD",
                "examples" : [
                  "2023-01-01"
                ],
                "format" : "date",
                "order" : 0
              },
              "should_load_today" : {
                "title" : "Загружать сегодня?",
                "type" : "boolean",
                "description" : "Загружать данные за сегодня? Если нет, последней датой загрузки будет вчера",
                "order" : 1,
                "default" : false
              },
              "date_range_type" : {
                "type" : "string",
                "title" : "Тип временного диапазона",
                "description" : "От даты начала до сегодня",
                "const" : "from_start_date_to_today",
                "order" : 2
              }
            }
          }
        ]
      },
      "group_by" : {
        "title" : "Тип группировки",
        "order" : 5,
        "type" : "object",
        "oneOf" : [
          {
            "title" : "По дням",
            "description" : "Группировка по дням",
            "properties" : {
              "group_type" : {
                "type" : "string",
                "const" : "day",
                "order" : 0
              }
            },
            "required" : [
              "group_type"
            ]
          },
          {
            "title" : "По неделям",
            "description" : "Группировка по неделям",
            "properties" : {
              "group_type" : {
                "type" : "string",
                "const" : "week",
                "order" : 0
              }
            },
            "required" : [
              "group_type"
            ]
          },
          {
            "title" : "По месяцам",
            "description" : "Группировка по месяцам",
            "properties" : {
              "group_type" : {
                "type" : "string",
                "const" : "month",
                "order" : 0
              }
            },
            "required" : [
              "group_type"
            ]
          }
        ]
      }
    }
  }
}